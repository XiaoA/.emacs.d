\label{sec-12-1-11-1}
\begin{itemize}
\item Log into app4, sudo to bbuser
\item Do a df -Th sh /usr/local/blackboard/tools/admin/RotateLogs.sh
\end{itemize}

\item Steps
\label{sec-12-1-11-2}
\begin{enumerate}
\item Do a df -Th 
\item If > 80\%, rotate logs
\item Sudo to bbuser
\item sh /usr/local/blackboard/tools/admin/RotateLogs.sh
\end{enumerate}
\end{itemize}
\subsubsection*{Update User Information}
\label{sec-12-1-12}

\begin{verbatim}
UPDATE BB_BB60.users SET <DATA> where USER_ID = 'buckinghama' COMMIT;
\end{verbatim}
\subsubsection*{Find Course PK1}
\label{sec-12-1-13}
\begin{verbatim}
select pk1 from bb_bb60.course_main where course_id like '%45551%'
\end{verbatim}
\subsubsection*{Find all available courses}
\label{sec-12-1-14}
\begin{verbatim}
select count(*) from BB_BB60.course_main where available_ind = 'Y'
\end{verbatim}
